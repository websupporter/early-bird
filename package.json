{
  "name": "cryptotraders-morning-briefing",
  "version": "1.0.0",
  "description": "An app which pulls in different data sources and aggregates the data to briefings useful for trading",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "ts-node src/migrations/run-migrations.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/websupporter/early-bird.git"
  },
  "keywords": ["crypto", "trading", "morning-briefing", "aggregation", "nodejs", "typescript"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/websupporter/early-bird/issues"
  },
  "homepage": "https://github.com/websupporter/early-bird#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "mysql2": "^3.6.0",
    "typeorm": "^0.3.17",
    "reflect-metadata": "^0.1.13",
    "dotenv": "^16.3.0",
    "axios": "^1.5.0",
    "winston": "^3.10.0",
    "cron": "^3.1.0",
    "rate-limiter-flexible": "^3.0.0",
    "openai": "^4.0.0",
    "snoowrap": "^1.23.0"
  }
}
